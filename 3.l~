{%
    enum {
        LOOKUP=0,
        VERB,
        ADJ,
        ADV,
        NOUN,
        PREP,
        PRON,
        CONJ
        };

    int state;

    int add_word(int type, char *word);
    int lookup_word(char *word);
%}

%%


\n { state = LOOKUP; }
^verb {state = ADJ; }
^adj { state = ADV; }
^noun { state = NOUN; }
^prep { state = PREP; }
^pron { state = PRON; }
^conj { state = CONJ; }

[a-zA-Z]+ {
