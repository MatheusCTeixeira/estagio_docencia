%%

%name Expr
%term NUM of int | ADD  | SUB  | MUL  | DIV | EOF | EOP | LPAR | RPAR
%nonterm f of int | t of int | EXPR of int | START of int
%pos int
%eop EOP

%left ADD SUB
%left MUL DIV

%start START
%noshift EOF

%%

START   : EXPR EOP      (print(EXPR))

EXPR    : NUM           (NUM)
        | EXPR ADD f (EXPR1 + EXPR2)
        | EXPR SUB f (EXPR1 - EXPR2)

f       :


t       : NUM (NUM1)
        | LPAR EXPR RPAR (EXPR1)
